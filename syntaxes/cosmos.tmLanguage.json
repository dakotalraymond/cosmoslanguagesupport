{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "COSMOS",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numeric-literal"
    },
    {
			"include": "#comment"
		},
		{
			"include": "#storage"
		},
		{
			"include": "#other"
		},
		{
			"include": "#constant"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.cosmos",
				"match": "\\b(INT|UINT|FLOAT|STRING|BLOCK|DERIVED|BIG_ENDIAN|CENTER|CONNECTOR|CONVERTED|DISABLED|DISCONNECT|ENABLED|EVENT|false|FALSE|FORMATTED|GREEN|HAZARDOUS|HOUSEKEEPING|HTML|LEFT|LINEGRAPH|LITTLE_ENDIAN|LOG|NO_CONNECTOR|NONE|ODD|PDF|RAW|READ|READ_WRITE|RED|RIGHT|SINGLEXY|SPLIT|true|TRUE|WITH_UNITS|WRITE|XY|YELLOW|TITLE|PACKET_LOG_WRITER|AUTO_INTERFACE_TARGETS|INTERFACE_TARGET|INTERFACE|ROUTER|COLLECT_METADATA|BACKGROUND_TASK|PARAMETER|ID_PARAMETER|ARRAY_PARAMETER|SELECT_PARAMETER|HIDDEN|DISABLED|DISABLE_MESSAGES|META|HAZARDOUS|COMMAND|SELECT_COMMAND|AUTO_START|AUTO_TARGET_COMPONENTS|TARGET_COMPONENT|COMPONENT|PAGE|TARGET_PAGES|ITEM|ITEM_ARRAY_INDEX|TIME_ITEM|FORMATTED_TIME_ITEM|VALUE_TYPE|ANALYSIS|ANALYSIS_SAMPLES|SHOW_LIMITS_LINES|TARGET|DONT_CONNECT|DONT_RECONNECT|RECONNECT_DELAY|DISABLE_DISCONNECT|LOG|DONT_LOG|LOG_RAW|PROTOCOL|OPTION|STATE|READ_CONVERSION|POLY_READ_CONVERSION|SEG_POLY_READ_CONVERSION|GENERIC_READ_CONVERSION_START|GENERIC_READ_CONVERSION_END|LIMITS|LIMITS_RESPONSE|TITLE|TOOL_FONT|LABEL_FONT|LABEL|DIVIDER|NUM_COLUMNS|TOOL|MULTITOOL_START|MULTITOOL_END|IGNORE_ITEM|IGNORE_PACKET|IGNORE_STALE|COLOR_BLIND|IGNORE_OPERATIONAL_LIMITS|HORIZONTAL_LINE|Y_OFFSET|Y_AXIS|TITLE|X_AXIS_TITLE|Y_AXIS_TITLE|SHOW_ABS_X_GRID_LABELS|SHOW_X_GRID_LINES|SHOW_Y_GRID_LINES|POINT_SIZE|SHOW_LINES|SHOW_LEGEND|MANUAL_Y_AXIS_SCALE|MANUAL_X_GRID_LINE_SCALE|MANUAL_Y_GRID_LINE_SCALE|UNIX_EPOCH_X_VALUES|UTC_TIME|NO_PDF|PDF_COVER|PDF_TOC|PDF_HEADER|PDF_FOOTER|PDF_TOP_MARGIN|PDF_BOTTOM_MARGIN|PDF_SIDE_MARGIN|TARGET|SECTION|CMD_SECTION|TLM_SECTION|REQUIRED|MINIMUM_VALUE|MAXIMUM_VALUE|DEFAULT_VALUE|STATE|WRITE_CONVERSION|POLY_WRITE_CONVERSION|SEG_POLY_WRITE_CONVERSION|GENERIC_WRITE_CONVERSION_START|GENERIC_WRITE_CONVERSION_END|OVERFLOW|FORMAT_STRING|UNITS|DESCRIPTION|META|BURST|FIXED|LENGTH|TERMINATED|PREIDENTIFIED|TEMPLATE|SCREEN|END|GLOBAL_SETTING|GLOBAL_SUBSETTING|SETTING|SUBSETTING|NAMED_WIDGET|LINE_DELAY|MONITOR_LIMITS|PAUSE_ON_RED|AUTO_DECLARE_TARGETS|DECLARE_TARGET|DECLARE_GEM_TARGET|PORT|LISTEN_HOST|CONNECT_HOST|PATH|DEFAULT_PACKET_LOG_WRITER|DEFAULT_PACKET_LOG_READER|STALENESS_SECONDS|ENABLE_DNS|DISABLE_DNS|ENABLE_SOUND|ALLOW_ACCESS|META_INIT|TIME_ZONE_UTC|ADD_MD|TABLEFILE|TABLE|HIDDEN|UNEDITABLE|REQUIRE|IGNORE_PARAMETER|IGNORE_ITEM|COMMANDS|TELEMETRY|AUTO_SCREEN_SUBSTITUTE|ITEM|APPEND_ITEM|ID_ITEM|APPEND_ID_ITEM|ARRAY_ITEM|APPEND_ARRAY_ITEM|SELECT_ITEM|META|PROCESSOR|ALLOW_SHORT|HIDDEN|TELEMETRY|SELECT_TELEMETRY|LIMITS_GROUP|LIMITS_GROUP_ITEM|LOAD_UTILITY|REQUIRE_UTILITY|RESULTS_WRITER|ALLOW_DEBUG|PAUSE_ON_ERROR|CONTINUE_TEST_CASE_AFTER_ERROR|ABORT_TESTING_AFTER_ERROR|MANUAL|LOOP_TESTING|BREAK_LOOP_AFTER_ERROR|IGNORE_TEST|IGNORE_TEST_SUITE|CREATE_DATA_PACKAGE|AUTO_CYCLE_LOGS|COLLECT_METADATA|DELIMITER|FILL_DOWN|DOWNSAMPLE_SECONDS|MATLAB_HEADER|UNIQUE_ONLY|UNIQUE_IGNORE|SHARE_COLUMNS|SHARE_COLUMN|FULL_COLUMN_NAMES|DONT_OUTPUT_FILENAMES|TEXT|ITEM|POINTS_SAVED|SECONDS_PLOTTED|POINTS_PLOTTED|REFRESH_RATE_HZ|CTS_TIMEOUT|TAB|AUTO_TARGETS|AUTO_TARGET|NEW_COLUMN|TARGET|ADD_SHOW_ON_STARTUP|GROUP|UNKNOWN|VERTICAL|VERTICALBOX|HORIZONTAL|HORIZONTALBOX|MATRIXBYCOLUMNS|SCROLLWINDOW|TABBOOK|TABITEM|LABEL|HORIZONTALLINE|SECTIONHEADER|TITLE|SPACER|ARRAY|BLOCK|FORMATFONTVALUE|FORMATVALUE|LABELPROGRESSBAR|LABELTRENDLIMITSBAR|LABELVALUE|LABELVALUEDESC|LABELVALUELIMITSBAR|LABELVALUELIMITSCOLUMN|LABELVALUERANGEBAR|LABELVALUERANGECOLUMN|LIMITSBAR|LIMITSCOLUMN|LIMITSCOLOR|VALUELIMITSBAR|VALUELIMITSCOLUMN|VALUERANGEBAR|VALUERANGECOLUMN|LINEGRAPH|PROGRESSBAR|RANGEBAR|RANGECOLUMN|TEXTBOX|TIMEGRAPH|TRENDBAR|TRENDLIMITSBAR|VALUE|BUTTON|CHECKBUTTON|COMBOBOX|RADIOBUTTON|TEXTFIELD|CANVAS|CANVASLABEL|CANVASLABELVALUE|CANVASIMAGE|CANVASIMAGEVALUE|CANVASLINE|CANVASLINEVALUE|CANVASDOT|TARGET|PACKET|X_ITEM|Y_ITEM|TIME_ITEM|X_VALUE_TYPE|Y_VALUE_TYPE|MANUAL_X_AXIS_SCALE)\\b"
			}]
		},
		"numeric-literal": {
			"patterns": [
				{
					"name": "constant.numeric.hex.cosmos",
					"match": "\\b0(x|X)[0-9a-fA-F_]+(U|u|L|l|UL|Ul|uL|ul|LU|Lu|lU|lu)?\\b"
				},
				{
					"name": "constant.numeric.binary.cosmos",
					"match": "\\b0(b|B)[01_]+(U|u|L|l|UL|Ul|uL|ul|LU|Lu|lU|lu)?\\b"
				},
				{
					"name": "constant.numeric.decimal.cosmos",
					"match": "\\b([0-9_]+)?\\.[0-9_]+((e|E)[0-9]+)?(F|f|D|d|M|m)?\\b"
				},
				{
					"name": "constant.numeric.decimal.cosmos",
					"match": "\\b[0-9_]+(e|E)[0-9_]+(F|f|D|d|M|m)?\\b"
				},
				{
					"name": "constant.numeric.decimal.cosmos",
					"match": "\\b[0-9_]+(F|f|D|d|M|m)\\b"
				},
				{
					"name": "constant.numeric.decimal.cosmos",
					"match": "\\b[0-9_]+(U|u|L|l|UL|Ul|uL|ul|LU|Lu|lU|lu)?\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.cosmos",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.cosmos",
					"match": "\\\\."
				}
			]
		},
		"comment": {
			"name": "comment.line.number-sign.cosmos",
			"begin": "(?<!/)#(?:(?!/)|(?=#))",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.comment.cosmos"
				}
			},
			"end": "(?=$)"
		},
		"storage": {
			"name": "storage.cosmos",
			"match": "\\b(TELEMETRY|COMMAND)\\b"
		},
		"other": {
			"patterns": [{
				"name": "entity.name.function",
				"match": "\\b(APPEND_ITEM|APPEND_ID_ITEM|APPEND_ARRAY_ITEM|APPEND_ID_PARAMETER|APPEND_PARAMETER)\\b"	
			}]
		},
		"constant": {
			"patterns": [{
				"name": "constant.language",
				"match": "\\b(TRUE|FALSE)\\b"
			}]
		}
	},
	"scopeName": "source.cosmos"
}